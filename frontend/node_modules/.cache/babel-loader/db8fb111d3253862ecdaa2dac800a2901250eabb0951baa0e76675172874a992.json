{"ast":null,"code":"import { ref, reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { useAuthStore } from '@/store/auth';\nimport Navbar from '@/components/layout/Navbar.vue';\nimport { signIn, signInWithGithub, signInWithGoogle } from 'supertokens-auth-react/recipe/thirdpartyemailpassword';\nexport default {\n  name: 'LoginPage',\n  components: {\n    Navbar\n  },\n  setup() {\n    const router = useRouter();\n    const authStore = useAuthStore();\n    const loginForm = ref(null);\n    const formData = reactive({\n      username: '',\n      password: '',\n      remember: false\n    });\n    const loading = ref(false);\n    const githubLoading = ref(false);\n    const googleLoading = ref(false);\n    const loginRules = {\n      username: [{\n        required: true,\n        message: '请输入用户名',\n        trigger: 'blur'\n      }, {\n        min: 3,\n        max: 20,\n        message: '用户名长度应为3-20个字符',\n        trigger: 'blur'\n      }],\n      password: [{\n        required: true,\n        message: '请输入密码',\n        trigger: 'blur'\n      }, {\n        min: 6,\n        message: '密码长度至少为6个字符',\n        trigger: 'blur'\n      }]\n    };\n    const handleLogin = async () => {\n      if (!loginForm.value) return;\n      await loginForm.value.validate(async valid => {\n        if (valid) {\n          try {\n            loading.value = true;\n            const response = await signIn({\n              formFields: [{\n                id: \"username\",\n                value: formData.username\n              }, {\n                id: \"password\",\n                value: formData.password\n              }]\n            });\n            if (response.status === \"OK\") {\n              ElMessage.success('登录成功');\n              router.push('/admin');\n            } else {\n              ElMessage.error('登录失败，请检查用户名和密码');\n            }\n          } catch (error) {\n            console.error('登录错误:', error);\n            ElMessage.error('登录失败，请稍后再试');\n          } finally {\n            loading.value = false;\n          }\n        }\n      });\n    };\n    const handleGithubLogin = async () => {\n      try {\n        githubLoading.value = true;\n        await signInWithGithub();\n      } catch (error) {\n        console.error('GitHub登录错误:', error);\n        ElMessage.error('GitHub登录失败，请稍后再试');\n      } finally {\n        githubLoading.value = false;\n      }\n    };\n    const handleGoogleLogin = async () => {\n      try {\n        googleLoading.value = true;\n        await signInWithGoogle();\n      } catch (error) {\n        console.error('Google登录错误:', error);\n        ElMessage.error('Google登录失败，请稍后再试');\n      } finally {\n        googleLoading.value = false;\n      }\n    };\n    const forgotPassword = () => {\n      router.push('/auth/reset-password');\n    };\n    const goToSignUp = () => {\n      router.push('/auth/sign-up');\n    };\n    return {\n      loginForm,\n      loginForm: formData,\n      loginRules,\n      loading,\n      githubLoading,\n      googleLoading,\n      handleLogin,\n      handleGithubLogin,\n      handleGoogleLogin,\n      forgotPassword,\n      goToSignUp\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","useRouter","ElMessage","useAuthStore","Navbar","signIn","signInWithGithub","signInWithGoogle","name","components","setup","router","authStore","loginForm","formData","username","password","remember","loading","githubLoading","googleLoading","loginRules","required","message","trigger","min","max","handleLogin","value","validate","valid","response","formFields","id","status","success","push","error","console","handleGithubLogin","handleGoogleLogin","forgotPassword","goToSignUp"],"sources":["C:\\Users\\Scott\\Downloads\\毕设\\frontend\\src\\views\\public\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-page\">\r\n    <Navbar />\r\n    \r\n    <div class=\"login-container\">\r\n      <div class=\"login-card\">\r\n        <div class=\"login-header\">\r\n          <h2>员工登录</h2>\r\n          <p>欢迎使用泓泰生物科技智能养殖管理系统</p>\r\n        </div>\r\n        \r\n        <el-form\r\n          ref=\"loginForm\"\r\n          :model=\"loginForm\"\r\n          :rules=\"loginRules\"\r\n          label-position=\"top\"\r\n          @submit.prevent=\"handleLogin\"\r\n        >\r\n          <el-form-item label=\"用户名\" prop=\"username\">\r\n            <el-input\r\n              v-model=\"loginForm.username\"\r\n              placeholder=\"请输入用户名\"\r\n              prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          \r\n          <el-form-item label=\"密码\" prop=\"password\">\r\n            <el-input\r\n              v-model=\"loginForm.password\"\r\n              type=\"password\"\r\n              placeholder=\"请输入密码\"\r\n              prefix-icon=\"Lock\"\r\n              show-password\r\n            />\r\n          </el-form-item>\r\n          \r\n          <div class=\"remember-forgot\">\r\n            <el-checkbox v-model=\"loginForm.remember\">记住我</el-checkbox>\r\n            <el-button type=\"text\" @click=\"forgotPassword\">忘记密码?</el-button>\r\n          </div>\r\n          \r\n          <el-form-item>\r\n            <el-button\r\n              type=\"primary\"\r\n              native-type=\"submit\"\r\n              :loading=\"loading\"\r\n              class=\"login-button\"\r\n            >\r\n              登录\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n        <div class=\"divider\">\r\n          <span>或使用以下方式登录</span>\r\n        </div>\r\n\r\n        <div class=\"oauth-buttons\">\r\n          <el-button\r\n            class=\"oauth-button github\"\r\n            :loading=\"githubLoading\"\r\n            @click=\"handleGithubLogin\"\r\n          >\r\n            <i class=\"fab fa-github\"></i>\r\n            使用GitHub登录\r\n          </el-button>\r\n\r\n          <el-button\r\n            class=\"oauth-button google\"\r\n            :loading=\"googleLoading\"\r\n            @click=\"handleGoogleLogin\"\r\n          >\r\n            <i class=\"fab fa-google\"></i>\r\n            使用Google登录\r\n          </el-button>\r\n        </div>\r\n        \r\n        <div class=\"login-footer\">\r\n          <p>\r\n            还没有账号？\r\n            <el-button type=\"text\" @click=\"goToSignUp\">立即注册</el-button>\r\n          </p>\r\n          <p>遇到问题? 请联系 <a href=\"mailto:support@hongtai-bio.com\">技术支持</a></p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { useAuthStore } from '@/store/auth'\r\nimport Navbar from '@/components/layout/Navbar.vue'\r\nimport { signIn, signInWithGithub, signInWithGoogle } from 'supertokens-auth-react/recipe/thirdpartyemailpassword'\r\n\r\nexport default {\r\n  name: 'LoginPage',\r\n  components: {\r\n    Navbar\r\n  },\r\n  \r\n  setup() {\r\n    const router = useRouter()\r\n    const authStore = useAuthStore()\r\n    const loginForm = ref(null)\r\n    \r\n    const formData = reactive({\r\n      username: '',\r\n      password: '',\r\n      remember: false\r\n    })\r\n    \r\n    const loading = ref(false)\r\n    const githubLoading = ref(false)\r\n    const googleLoading = ref(false)\r\n    \r\n    const loginRules = {\r\n      username: [\r\n        { required: true, message: '请输入用户名', trigger: 'blur' },\r\n        { min: 3, max: 20, message: '用户名长度应为3-20个字符', trigger: 'blur' }\r\n      ],\r\n      password: [\r\n        { required: true, message: '请输入密码', trigger: 'blur' },\r\n        { min: 6, message: '密码长度至少为6个字符', trigger: 'blur' }\r\n      ]\r\n    }\r\n    \r\n    const handleLogin = async () => {\r\n      if (!loginForm.value) return\r\n      \r\n      await loginForm.value.validate(async (valid) => {\r\n        if (valid) {\r\n          try {\r\n            loading.value = true\r\n            const response = await signIn({\r\n              formFields: [\r\n                {\r\n                  id: \"username\",\r\n                  value: formData.username\r\n                },\r\n                {\r\n                  id: \"password\",\r\n                  value: formData.password\r\n                }\r\n              ]\r\n            })\r\n            \r\n            if (response.status === \"OK\") {\r\n              ElMessage.success('登录成功')\r\n              router.push('/admin')\r\n            } else {\r\n              ElMessage.error('登录失败，请检查用户名和密码')\r\n            }\r\n          } catch (error) {\r\n            console.error('登录错误:', error)\r\n            ElMessage.error('登录失败，请稍后再试')\r\n          } finally {\r\n            loading.value = false\r\n          }\r\n        }\r\n      })\r\n    }\r\n    \r\n    const handleGithubLogin = async () => {\r\n      try {\r\n        githubLoading.value = true\r\n        await signInWithGithub()\r\n      } catch (error) {\r\n        console.error('GitHub登录错误:', error)\r\n        ElMessage.error('GitHub登录失败，请稍后再试')\r\n      } finally {\r\n        githubLoading.value = false\r\n      }\r\n    }\r\n    \r\n    const handleGoogleLogin = async () => {\r\n      try {\r\n        googleLoading.value = true\r\n        await signInWithGoogle()\r\n      } catch (error) {\r\n        console.error('Google登录错误:', error)\r\n        ElMessage.error('Google登录失败，请稍后再试')\r\n      } finally {\r\n        googleLoading.value = false\r\n      }\r\n    }\r\n    \r\n    const forgotPassword = () => {\r\n      router.push('/auth/reset-password')\r\n    }\r\n\r\n    const goToSignUp = () => {\r\n      router.push('/auth/sign-up')\r\n    }\r\n    \r\n    return {\r\n      loginForm,\r\n      loginForm: formData,\r\n      loginRules,\r\n      loading,\r\n      githubLoading,\r\n      googleLoading,\r\n      handleLogin,\r\n      handleGithubLogin,\r\n      handleGoogleLogin,\r\n      forgotPassword,\r\n      goToSignUp\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-page {\r\n  min-height: 100vh;\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 60px 20px;\r\n}\r\n\r\n.login-card {\r\n  width: 100%;\r\n  max-width: 450px;\r\n  background-color: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\r\n  padding: 40px;\r\n}\r\n\r\n.login-header {\r\n  text-align: center;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.login-header h2 {\r\n  font-size: 1.8rem;\r\n  color: #333;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.login-header p {\r\n  color: #666;\r\n  font-size: 1rem;\r\n}\r\n\r\n.remember-forgot {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  padding: 12px 0;\r\n  font-size: 1.1rem;\r\n}\r\n\r\n.divider {\r\n  display: flex;\r\n  align-items: center;\r\n  margin: 20px 0;\r\n  text-align: center;\r\n}\r\n\r\n.divider::before,\r\n.divider::after {\r\n  content: '';\r\n  flex: 1;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n\r\n.divider span {\r\n  padding: 0 10px;\r\n  color: #999;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.oauth-buttons {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.oauth-button {\r\n  width: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 10px;\r\n  font-size: 1rem;\r\n  height: 40px;\r\n}\r\n\r\n.oauth-button.github {\r\n  background-color: #24292e;\r\n  color: white;\r\n}\r\n\r\n.oauth-button.google {\r\n  background-color: #fff;\r\n  color: #757575;\r\n  border: 1px solid #ddd;\r\n}\r\n\r\n.login-footer {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  color: #999;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.login-footer a {\r\n  color: #409EFF;\r\n  text-decoration: none;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .login-card {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .login-header h2 {\r\n    font-size: 1.5rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA0FA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,SAAQ,QAAS,cAAa;AACvC,SAASC,YAAW,QAAS,cAAa;AAC1C,OAAOC,MAAK,MAAO,gCAA+B;AAClD,SAASC,MAAM,EAAEC,gBAAgB,EAAEC,gBAAe,QAAS,uDAAsD;AAEjH,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVL;EACF,CAAC;EAEDM,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIV,SAAS,CAAC;IACzB,MAAMW,SAAQ,GAAIT,YAAY,CAAC;IAC/B,MAAMU,SAAQ,GAAId,GAAG,CAAC,IAAI;IAE1B,MAAMe,QAAO,GAAId,QAAQ,CAAC;MACxBe,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;IAED,MAAMC,OAAM,GAAInB,GAAG,CAAC,KAAK;IACzB,MAAMoB,aAAY,GAAIpB,GAAG,CAAC,KAAK;IAC/B,MAAMqB,aAAY,GAAIrB,GAAG,CAAC,KAAK;IAE/B,MAAMsB,UAAS,GAAI;MACjBN,QAAQ,EAAE,CACR;QAAEO,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,QAAQ;QAAEC,OAAO,EAAE;MAAO,CAAC,EACtD;QAAEC,GAAG,EAAE,CAAC;QAAEC,GAAG,EAAE,EAAE;QAAEH,OAAO,EAAE,gBAAgB;QAAEC,OAAO,EAAE;MAAO,EAC/D;MACDR,QAAQ,EAAE,CACR;QAAEM,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,OAAO;QAAEC,OAAO,EAAE;MAAO,CAAC,EACrD;QAAEC,GAAG,EAAE,CAAC;QAAEF,OAAO,EAAE,aAAa;QAAEC,OAAO,EAAE;MAAO;IAEtD;IAEA,MAAMG,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI,CAACd,SAAS,CAACe,KAAK,EAAE;MAEtB,MAAMf,SAAS,CAACe,KAAK,CAACC,QAAQ,CAAC,MAAOC,KAAK,IAAK;QAC9C,IAAIA,KAAK,EAAE;UACT,IAAI;YACFZ,OAAO,CAACU,KAAI,GAAI,IAAG;YACnB,MAAMG,QAAO,GAAI,MAAM1B,MAAM,CAAC;cAC5B2B,UAAU,EAAE,CACV;gBACEC,EAAE,EAAE,UAAU;gBACdL,KAAK,EAAEd,QAAQ,CAACC;cAClB,CAAC,EACD;gBACEkB,EAAE,EAAE,UAAU;gBACdL,KAAK,EAAEd,QAAQ,CAACE;cAClB;YAEJ,CAAC;YAED,IAAIe,QAAQ,CAACG,MAAK,KAAM,IAAI,EAAE;cAC5BhC,SAAS,CAACiC,OAAO,CAAC,MAAM;cACxBxB,MAAM,CAACyB,IAAI,CAAC,QAAQ;YACtB,OAAO;cACLlC,SAAS,CAACmC,KAAK,CAAC,gBAAgB;YAClC;UACF,EAAE,OAAOA,KAAK,EAAE;YACdC,OAAO,CAACD,KAAK,CAAC,OAAO,EAAEA,KAAK;YAC5BnC,SAAS,CAACmC,KAAK,CAAC,YAAY;UAC9B,UAAU;YACRnB,OAAO,CAACU,KAAI,GAAI,KAAI;UACtB;QACF;MACF,CAAC;IACH;IAEA,MAAMW,iBAAgB,GAAI,MAAAA,CAAA,KAAY;MACpC,IAAI;QACFpB,aAAa,CAACS,KAAI,GAAI,IAAG;QACzB,MAAMtB,gBAAgB,CAAC;MACzB,EAAE,OAAO+B,KAAK,EAAE;QACdC