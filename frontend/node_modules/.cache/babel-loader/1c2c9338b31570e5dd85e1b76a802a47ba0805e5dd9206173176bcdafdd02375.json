{"ast":null,"code":"import { ref, reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport Navbar from '@/components/layout/Navbar.vue';\nimport { signUp, signUpWithGithub, signUpWithGoogle } from 'supertokens-auth-react/recipe/thirdpartyemailpassword';\nexport default {\n  name: 'SignUpPage',\n  components: {\n    Navbar\n  },\n  setup() {\n    const router = useRouter();\n    const signupForm = ref(null);\n    const formData = reactive({\n      username: '',\n      email: '',\n      password: '',\n      confirmPassword: ''\n    });\n    const loading = ref(false);\n    const githubLoading = ref(false);\n    const googleLoading = ref(false);\n    const validatePass = (rule, value, callback) => {\n      if (value === '') {\n        callback(new Error('请输入密码'));\n      } else {\n        if (formData.confirmPassword !== '') {\n          signupForm.value.validateField('confirmPassword');\n        }\n        callback();\n      }\n    };\n    const validatePass2 = (rule, value, callback) => {\n      if (value === '') {\n        callback(new Error('请再次输入密码'));\n      } else if (value !== formData.password) {\n        callback(new Error('两次输入密码不一致'));\n      } else {\n        callback();\n      }\n    };\n    const signupRules = {\n      username: [{\n        required: true,\n        message: '请输入用户名',\n        trigger: 'blur'\n      }, {\n        min: 3,\n        max: 20,\n        message: '用户名长度应为3-20个字符',\n        trigger: 'blur'\n      }],\n      email: [{\n        required: true,\n        message: '请输入邮箱地址',\n        trigger: 'blur'\n      }, {\n        type: 'email',\n        message: '请输入正确的邮箱地址',\n        trigger: 'blur'\n      }],\n      password: [{\n        required: true,\n        validator: validatePass,\n        trigger: 'blur'\n      }, {\n        min: 6,\n        message: '密码长度至少为6个字符',\n        trigger: 'blur'\n      }],\n      confirmPassword: [{\n        required: true,\n        validator: validatePass2,\n        trigger: 'blur'\n      }]\n    };\n    const handleSignUp = async () => {\n      if (!signupForm.value) return;\n      await signupForm.value.validate(async valid => {\n        if (valid) {\n          try {\n            loading.value = true;\n            const response = await signUp({\n              formFields: [{\n                id: \"username\",\n                value: formData.username\n              }, {\n                id: \"email\",\n                value: formData.email\n              }, {\n                id: \"password\",\n                value: formData.password\n              }]\n            });\n            if (response.status === \"OK\") {\n              ElMessage.success('注册成功');\n              router.push('/login');\n            } else {\n              ElMessage.error(response.message || '注册失败');\n            }\n          } catch (error) {\n            console.error('注册错误:', error);\n            ElMessage.error('注册失败，请稍后再试');\n          } finally {\n            loading.value = false;\n          }\n        }\n      });\n    };\n    const handleGithubSignUp = async () => {\n      try {\n        githubLoading.value = true;\n        await signUpWithGithub();\n      } catch (error) {\n        console.error('GitHub注册错误:', error);\n        ElMessage.error('GitHub注册失败，请稍后再试');\n      } finally {\n        githubLoading.value = false;\n      }\n    };\n    const handleGoogleSignUp = async () => {\n      try {\n        googleLoading.value = true;\n        await signUpWithGoogle();\n      } catch (error) {\n        console.error('Google注册错误:', error);\n        ElMessage.error('Google注册失败，请稍后再试');\n      } finally {\n        googleLoading.value = false;\n      }\n    };\n    const goToLogin = () => {\n      router.push('/login');\n    };\n    return {\n      signupForm,\n      signupForm: formData,\n      signupRules,\n      loading,\n      githubLoading,\n      googleLoading,\n      handleSignUp,\n      handleGithubSignUp,\n      handleGoogleSignUp,\n      goToLogin\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","useRouter","ElMessage","Navbar","signUp","signUpWithGithub","signUpWithGoogle","name","components","setup","router","signupForm","formData","username","email","password","confirmPassword","loading","githubLoading","googleLoading","validatePass","rule","value","callback","Error","validateField","validatePass2","signupRules","required","message","trigger","min","max","type","validator","handleSignUp","validate","valid","response","formFields","id","status","success","push","error","console","handleGithubSignUp","handleGoogleSignUp","goToLogin"],"sources":["C:\\Users\\Scott\\Downloads\\毕设\\frontend\\src\\views\\public\\SignUp.vue"],"sourcesContent":["<template>\r\n  <div class=\"signup-page\">\r\n    <Navbar />\r\n    \r\n    <div class=\"signup-container\">\r\n      <div class=\"signup-card\">\r\n        <div class=\"signup-header\">\r\n          <h2>注册账号</h2>\r\n          <p>加入泓泰生物科技智能养殖管理系统</p>\r\n        </div>\r\n        \r\n        <el-form\r\n          ref=\"signupForm\"\r\n          :model=\"signupForm\"\r\n          :rules=\"signupRules\"\r\n          label-position=\"top\"\r\n          @submit.prevent=\"handleSignUp\"\r\n        >\r\n          <el-form-item label=\"用户名\" prop=\"username\">\r\n            <el-input\r\n              v-model=\"signupForm.username\"\r\n              placeholder=\"请输入用户名\"\r\n              prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          \r\n          <el-form-item label=\"邮箱\" prop=\"email\">\r\n            <el-input\r\n              v-model=\"signupForm.email\"\r\n              type=\"email\"\r\n              placeholder=\"请输入邮箱\"\r\n              prefix-icon=\"Message\"\r\n            />\r\n          </el-form-item>\r\n          \r\n          <el-form-item label=\"密码\" prop=\"password\">\r\n            <el-input\r\n              v-model=\"signupForm.password\"\r\n              type=\"password\"\r\n              placeholder=\"请输入密码\"\r\n              prefix-icon=\"Lock\"\r\n              show-password\r\n            />\r\n          </el-form-item>\r\n          \r\n          <el-form-item label=\"确认密码\" prop=\"confirmPassword\">\r\n            <el-input\r\n              v-model=\"signupForm.confirmPassword\"\r\n              type=\"password\"\r\n              placeholder=\"请再次输入密码\"\r\n              prefix-icon=\"Lock\"\r\n              show-password\r\n            />\r\n          </el-form-item>\r\n          \r\n          <el-form-item>\r\n            <el-button\r\n              type=\"primary\"\r\n              native-type=\"submit\"\r\n              :loading=\"loading\"\r\n              class=\"signup-button\"\r\n            >\r\n              注册\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n        <div class=\"divider\">\r\n          <span>或使用以下方式注册</span>\r\n        </div>\r\n\r\n        <div class=\"oauth-buttons\">\r\n          <el-button\r\n            class=\"oauth-button github\"\r\n            :loading=\"githubLoading\"\r\n            @click=\"handleGithubSignUp\"\r\n          >\r\n            <i class=\"fab fa-github\"></i>\r\n            使用GitHub注册\r\n          </el-button>\r\n\r\n          <el-button\r\n            class=\"oauth-button google\"\r\n            :loading=\"googleLoading\"\r\n            @click=\"handleGoogleSignUp\"\r\n          >\r\n            <i class=\"fab fa-google\"></i>\r\n            使用Google注册\r\n          </el-button>\r\n        </div>\r\n        \r\n        <div class=\"signup-footer\">\r\n          <p>\r\n            已有账号？\r\n            <el-button type=\"text\" @click=\"goToLogin\">立即登录</el-button>\r\n          </p>\r\n          <p>注册即表示同意<a href=\"/terms\">服务条款</a>和<a href=\"/privacy\">隐私政策</a></p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport Navbar from '@/components/layout/Navbar.vue'\r\nimport { signUp, signUpWithGithub, signUpWithGoogle } from 'supertokens-auth-react/recipe/thirdpartyemailpassword'\r\n\r\nexport default {\r\n  name: 'SignUpPage',\r\n  components: {\r\n    Navbar\r\n  },\r\n  \r\n  setup() {\r\n    const router = useRouter()\r\n    const signupForm = ref(null)\r\n    \r\n    const formData = reactive({\r\n      username: '',\r\n      email: '',\r\n      password: '',\r\n      confirmPassword: ''\r\n    })\r\n    \r\n    const loading = ref(false)\r\n    const githubLoading = ref(false)\r\n    const googleLoading = ref(false)\r\n    \r\n    const validatePass = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请输入密码'))\r\n      } else {\r\n        if (formData.confirmPassword !== '') {\r\n          signupForm.value.validateField('confirmPassword')\r\n        }\r\n        callback()\r\n      }\r\n    }\r\n    \r\n    const validatePass2 = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请再次输入密码'))\r\n      } else if (value !== formData.password) {\r\n        callback(new Error('两次输入密码不一致'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    \r\n    const signupRules = {\r\n      username: [\r\n        { required: true, message: '请输入用户名', trigger: 'blur' },\r\n        { min: 3, max: 20, message: '用户名长度应为3-20个字符', trigger: 'blur' }\r\n      ],\r\n      email: [\r\n        { required: true, message: '请输入邮箱地址', trigger: 'blur' },\r\n        { type: 'email', message: '请输入正确的邮箱地址', trigger: 'blur' }\r\n      ],\r\n      password: [\r\n        { required: true, validator: validatePass, trigger: 'blur' },\r\n        { min: 6, message: '密码长度至少为6个字符', trigger: 'blur' }\r\n      ],\r\n      confirmPassword: [\r\n        { required: true, validator: validatePass2, trigger: 'blur' }\r\n      ]\r\n    }\r\n    \r\n    const handleSignUp = async () => {\r\n      if (!signupForm.value) return\r\n      \r\n      await signupForm.value.validate(async (valid) => {\r\n        if (valid) {\r\n          try {\r\n            loading.value = true\r\n            const response = await signUp({\r\n              formFields: [\r\n                {\r\n                  id: \"username\",\r\n                  value: formData.username\r\n                },\r\n                {\r\n                  id: \"email\",\r\n                  value: formData.email\r\n                },\r\n                {\r\n                  id: \"password\",\r\n                  value: formData.password\r\n                }\r\n              ]\r\n            })\r\n            \r\n            if (response.status === \"OK\") {\r\n              ElMessage.success('注册成功')\r\n              router.push('/login')\r\n            } else {\r\n              ElMessage.error(response.message || '注册失败')\r\n            }\r\n          } catch (error) {\r\n            console.error('注册错误:', error)\r\n            ElMessage.error('注册失败，请稍后再试')\r\n          } finally {\r\n            loading.value = false\r\n          }\r\n        }\r\n      })\r\n    }\r\n    \r\n    const handleGithubSignUp = async () => {\r\n      try {\r\n        githubLoading.value = true\r\n        await signUpWithGithub()\r\n      } catch (error) {\r\n        console.error('GitHub注册错误:', error)\r\n        ElMessage.error('GitHub注册失败，请稍后再试')\r\n      } finally {\r\n        githubLoading.value = false\r\n      }\r\n    }\r\n    \r\n    const handleGoogleSignUp = async () => {\r\n      try {\r\n        googleLoading.value = true\r\n        await signUpWithGoogle()\r\n      } catch (error) {\r\n        console.error('Google注册错误:', error)\r\n        ElMessage.error('Google注册失败，请稍后再试')\r\n      } finally {\r\n        googleLoading.value = false\r\n      }\r\n    }\r\n    \r\n    const goToLogin = () => {\r\n      router.push('/login')\r\n    }\r\n    \r\n    return {\r\n      signupForm,\r\n      signupForm: formData,\r\n      signupRules,\r\n      loading,\r\n      githubLoading,\r\n      googleLoading,\r\n      handleSignUp,\r\n      handleGithubSignUp,\r\n      handleGoogleSignUp,\r\n      goToLogin\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.signup-page {\r\n  min-height: 100vh;\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.signup-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 60px 20px;\r\n}\r\n\r\n.signup-card {\r\n  width: 100%;\r\n  max-width: 450px;\r\n  background-color: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\r\n  padding: 40px;\r\n}\r\n\r\n.signup-header {\r\n  text-align: center;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.signup-header h2 {\r\n  font-size: 1.8rem;\r\n  color: #333;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.signup-header p {\r\n  color: #666;\r\n  font-size: 1rem;\r\n}\r\n\r\n.signup-button {\r\n  width: 100%;\r\n  padding: 12px 0;\r\n  font-size: 1.1rem;\r\n}\r\n\r\n.divider {\r\n  display: flex;\r\n  align-items: center;\r\n  margin: 20px 0;\r\n  text-align: center;\r\n}\r\n\r\n.divider::before,\r\n.divider::after {\r\n  content: '';\r\n  flex: 1;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n\r\n.divider span {\r\n  padding: 0 10px;\r\n  color: #999;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.oauth-buttons {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.oauth-button {\r\n  width: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 10px;\r\n  font-size: 1rem;\r\n  height: 40px;\r\n}\r\n\r\n.oauth-button.github {\r\n  background-color: #24292e;\r\n  color: white;\r\n}\r\n\r\n.oauth-button.google {\r\n  background-color: #fff;\r\n  color: #757575;\r\n  border: 1px solid #ddd;\r\n}\r\n\r\n.signup-footer {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  color: #999;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.signup-footer a {\r\n  color: #409EFF;\r\n  text-decoration: none;\r\n}